{
    "swagger": "2.0",
    "info": {
        "description": "TON Emulate API provides an endpoint to emulate transactions and traces before committing them to the blockchain.",
        "title": "TON Emulate API",
        "contact": {},
        "version": "0.0.1"
    },
    "basePath": "/api/emulate/",
    "paths": {
        "/v1/emulateTrace": {
            "post": {
                "description": "Emulate trace by external message.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "emulate"
                ],
                "summary": "Emulate trace by external message",
                "parameters": [
                    {
                        "description": "External Message Request",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/main.EmulateRequest"
                        }
                    }
                ],
                "responses": {}
            }
        }
    },
    "definitions": {
        "main.EmulateRequest": {
            "type": "object",
            "properties": {
                "boc": {
                    "type": "string",
                    "example": "te6ccgEBAQEAAgAAAA=="
                },
                "ignore_chksig": {
                    "type": "boolean",
                    "example": false
                },
                "include_code_data": {
                    "type": "boolean",
                    "example": false
                },
                "mc_block_seqno": {
                    "type": "integer",
                    "example": null,
                    "nullable": true
                },
                "with_actions": {
                    "type": "boolean",
                    "example": false
                }
            }
        }
    }
}